export PATH=/usr/local/sbin:/usr/local/bin:$HOME/.rvm/bin:/usr/local/share/npm/bin:$(brew --prefix php54)/bin:$(echo $PATH | sed 's/:\/usr\/local\/bin//')

export ARCHFLAGS="-arch x86_64"
export DEFAULT_USER="anthony"

